<workunit schemaName="master_tbljobposition">
    <invokers>
        <invoker type="master_tbljobposition:create" caption="New Job Position" action="create" target="popup"/>
        <invoker type="master_tbljobposition:open" caption="Job Position" action="open"/>
        <invoker type="formActions" caption="Approve" action="approve" 
                 visibleWhen="#{allowApprove==true}"/>
    </invokers>
    <code class="tagabukid.hrmis.appointment.models.MasterJobPositionModel" />
    <pages>
        <page template="tagabukid.hrmis.appointment.views.MasterJobPositionView" />
    </pages>
</workunit>
