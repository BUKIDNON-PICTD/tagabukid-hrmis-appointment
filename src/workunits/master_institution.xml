<workunit schemaName="master_tblinstitution">
    <invokers>
        <invoker type="master_tblinstitution:create" caption="New Institution" action="create" target="popup" role="MASTER"/>
        <invoker type="master_tblinstitution:open" caption="Institution" action="open" target="popup" role="MASTER"/>
        <invoker type="formActions" caption="Approve" action="approve" 
                 visibleWhen="#{allowApprove==true}"/>
    </invokers>
    <code class="tagabukid.hrmis.appointment.models.MasterInstitutionModel" />
    <pages>
        <page template="tagabukid.hrmis.appointment.views.MasterInstitutionView"/>
    </pages>
</workunit>
