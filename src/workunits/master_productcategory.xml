<workunit schemaName="master_tblproductcategory">
    <invokers>
        <invoker type="master_tblproductcategory:create" caption="New Product Category" action="create" target="popup" role="MASTER"/>
        <invoker type="master_tblproductcategory:open" caption="Product Category" action="open" target="popup" role="MASTER"/>
        <invoker type="formActions" caption="Approve" action="approve" 
                 visibleWhen="#{allowApprove==true}"/>
    </invokers>
    <code class="tagabukid.hrmis.appointment.models.MasterProductCategoryModel" />
    <pages>
        <page template="tagabukid.hrmis.appointment.views.MasterProductCategoryView"/>
    </pages>
</workunit>
